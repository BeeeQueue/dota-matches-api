{
  "name": "@beequeue/dota-matches-api",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "CC-BY-SA-3.0-US",
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "node": ">=20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "wrangler dev --local",
    "dev:migrate": "wrangler d1 migrations apply --local dpc",
    "build": "tsup",
    "lint": "eslint src",
    "test": "vitest --mode test",
    "test:e2e": "vitest --mode test --config vitest.e2e.config.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "4.0.0",
    "@cloudflare/workers-types": "4.20231025.0",
    "@leeoniya/ufuzzy": "1.0.11",
    "@total-typescript/ts-reset": "0.5.1",
    "@tsconfig/node20": "20.1.2",
    "@types/node": "20.8.9",
    "@worker-tools/response-creators": "1.2.0-pre.6",
    "better-sqlite3": "8.7.0",
    "date-fns": "2.30.0",
    "discord-api-types": "0.37.61",
    "discord-interactions": "3.4.0",
    "dotenv": "16.3.1",
    "drizzle-kit": "0.19.13",
    "drizzle-orm": "0.28.6",
    "eslint": "8.52.0",
    "itty-cors": "0.3.6",
    "itty-router": "4.0.23",
    "lint-staged": "15.0.2",
    "mande": "2.0.8",
    "miniflare": "3.20231025.0",
    "ms": "3.0.0-canary.1",
    "nanoid": "5.0.2",
    "node-html-parser": "6.1.11",
    "p-queue": "7.4.1",
    "prettier": "3.0.3",
    "remeda": "1.28.0",
    "simple-git-hooks": "2.9.0",
    "tsup": "7.2.0",
    "typescript": "5.2.2",
    "undici-types": "5.27.0",
    "vitest": "0.34.6",
    "vitest-environment-miniflare": "2.14.1",
    "wrangler": "3.15.0"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@latest",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@latest",
      "has": "npm:@nolyfill/has@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.entries": "npm:@nolyfill/object.entries@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.hasown": "npm:@nolyfill/object.hasown@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@latest"
    }
  }
}
