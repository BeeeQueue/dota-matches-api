    "nanoid": "4.0.0",
    "node-html-parser": "6.1.4",
    "p-queue": "7.3.0",
    "prettier": "2.8.1",
    "remeda": "1.3.0",
    "simple-git-hooks": "2.8.1",
    "tsup": "6.5.0",
    "typescript": "4.9.4",
