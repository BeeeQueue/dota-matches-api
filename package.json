    "date-fns": "2.29.3",
    "discord-api-types": "0.37.21",
    "discord-interactions": "3.2.0",
    "dotenv": "16.0.3",
    "esbuild": "0.16.4",
    "eslint": "8.29.0",
    "fuse.js": "6.6.2",
    "itty-router": "2.6.6",
