{
  "name": "@beequeue/dota-matches-api",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "CC-BY-SA-3.0-US",
  "private": true,
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "wrangler dev --local",
    "dev:migrate": "wrangler d1 migrations apply --local dpc",
    "build": "tsup",
    "lint": "eslint src",
    "test": "vitest --mode test",
    "test:e2e": "vitest --mode test --config vitest.e2e.config.ts",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "2.1.1",
    "@cloudflare/workers-types": "4.20231010.0",
    "@total-typescript/ts-reset": "0.4.2",
    "@tsconfig/node20": "20.1.2",
    "@types/node": "20.3.1",
    "@worker-tools/response-creators": "1.2.0-pre.6",
    "better-sqlite3": "8.4.0",
    "date-fns": "2.30.0",
    "discord-api-types": "0.37.46",
    "discord-interactions": "3.4.0",
    "dotenv": "16.3.1",
    "eslint": "8.50.0",
    "fuse.js": "6.6.2",
    "itty-cors": "0.3.6",
    "itty-router": "4.0.13",
    "kysely": "0.25.0",
    "kysely-d1": "0.3.0",
    "lint-staged": "13.3.0",
    "mande": "2.0.6",
    "miniflare": "3.20231010.0",
    "ms": "3.0.0-canary.1",
    "nanoid": "5.0.1",
    "node-html-parser": "6.1.5",
    "p-queue": "7.3.4",
    "prettier": "2.8.8",
    "remeda": "1.19.0",
    "simple-git-hooks": "2.8.1",
    "tsup": "7.0.0",
    "typescript": "5.1.3",
    "undici": "5.22.1",
    "vite": "4.4.11",
    "vitest": "0.34.6",
    "vitest-environment-miniflare": "2.14.1",
    "wrangler": "3.12.0"
  }
}
